{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_KAKAO_CLIENT_ID": "@vite_kakao_client_id",
    "VITE_KAKAO_REDIRECT_URL": "@vite_kakao_redirect_url"
  }
}
